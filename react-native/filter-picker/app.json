{
  "expo": {
    "name": "filterpicker",
    "slug": "filterpicker",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "todo",
        "NSCameraUsageDescription": "todo",
        "NSPhotoLibraryAddUsageDescription": "todo",
        "NSMicrophoneUsageDescription": "todo"
      },
      "bundleIdentifier": "filterpicker"
    },
    "android": {
      "package": "filter.picker"
    },
    "entryPoint": "node_modules/expo/AppEntry.js"
  }
}